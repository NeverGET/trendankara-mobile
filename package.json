{
  "name": "mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npx patch-package",
    "start": "expo start",
    "start:clear": "expo start --clear",
    "start:dev-client": "expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "android:dev": "expo run:android --variant debug",
    "android:release": "expo run:android --variant release",
    "android:install": "./scripts/android-install.sh",
    "android:install-clean": "CLEAN_INSTALL=true ./scripts/android-install.sh",
    "android:build": "eas build --platform android",
    "android:build:local": "eas build --platform android --local",
    "ios": "expo run:ios",
    "ios:dev": "expo run:ios --configuration Debug",
    "ios:release": "expo run:ios --configuration Release",
    "ios:device": "expo run:ios --device",
    "ios:iphone": "expo run:ios --device \"Muhammed Zülfü iPhone'u\"",
    "ios:build": "eas build --platform ios",
    "ios:build:local": "eas build --platform ios --local",
    "web": "expo start --web",
    "web:build": "expo export --platform web",
    "build": "eas build --platform all",
    "build:local": "eas build --platform all --local",
    "build:preview": "eas build --platform all --profile preview",
    "build:production": "eas build --platform all --profile production",
    "submit": "eas submit --platform all",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "update": "eas update",
    "update:preview": "eas update --branch preview",
    "update:production": "eas update --branch production",
    "generate-icons": "npx tsx scripts/generate-icons.ts",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@reduxjs/toolkit": "^2.9.0",
    "axios": "^1.12.2",
    "expo": "54.0.13",
    "expo-audio": "^1.0.13",
    "expo-av": "~16.0.7",
    "expo-battery": "^10.0.7",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.15",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.9",
    "expo-linking": "~8.0.8",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.12",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-store-review": "~9.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-task-manager": "14.0.7",
    "expo-video": "^3.0.11",
    "expo-web-browser": "~15.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-track-player": "^4.1.2",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.0",
    "ansi-regex": "^5.0.1",
    "commander": "^11.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "jest-environment-node": "^30.1.2",
    "patch-package": "^8.0.0",
    "react-test-renderer": "^19.1.0",
    "sharp": "^0.33.0",
    "svgson": "^5.3.0",
    "ts-jest": "^29.4.4",
    "typescript": "~5.9.2"
  },
  "private": true,
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/__tests__/**/*.test.tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "collectCoverageFrom": [
      "services/**/*.{ts,tsx}",
      "components/**/*.{ts,tsx}",
      "hooks/**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!**/*.test.{ts,tsx}"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@testing-library)/)"
    ]
  }
}
